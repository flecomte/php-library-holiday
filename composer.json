{
    "name": "michalmanko/php-library-holiday",
    "version": "1.1.0",
    "description": "A library to calculate holidays",
    "keywords": ["holiday", "polish holidays", "danish holidays", "french holidays"],
    "type": "library",
    "license": "MIT",
    "authors": [{
            "name": "Michał Mańko",
            "email": "github@michalmanko.com"
        }],
    "autoload": {
        "psr-4": {
            "Michalmanko\\Holiday\\": "src/Michalmanko/Holiday/",
            "Michalmanko\\Holiday\\Test\\": "tests/Michalmanko/Holiday/Test/"
        }
    },
    "require": {
        "php": ">=5.3.23"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6.0",
        "johnkary/phpunit-speedtrap": "1.0.*@dev",
        "fabpot/php-cs-fixer": "~1.8.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
